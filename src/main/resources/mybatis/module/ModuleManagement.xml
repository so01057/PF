<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zemnnni.common.module.repository.ModuleManagementRepository">
    <select id="selectModuleManagementList" resultType="com.zemnnni.common.module.dto.ModuleManagementResponse">
        SELECT
            module_id
            ,module_name
            ,module_type_code
            ,module_description
            ,module_use_yn
        FROM module_management
    </select>
</mapper>